<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moirai.alloc.gantt.query.mapper.ProjectQueryMapper">
    <select id="findProjectPeriod" resultType="com.moirai.alloc.common.port.ProjectPeriod">
        SELECT
            start_date AS startDate,
            end_date AS endDate
        FROM project
        WHERE project_id = #{projectId}
    </select>
</mapper>
